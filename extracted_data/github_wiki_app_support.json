  "supported_features": {
    "server_sent_events": [
      "CONVERSATION_MESSAGE",
      "CONVERSATION_PARTICIPANT_CHANGED", 
      "CONVERSATION_ROUTING_RESULT",
      "CONVERSATION_DELIVERY_ACKNOWLEDGEMENT",
      "CONVERSATION_READ_ACKNOWLEDGEMENT", 
      "CONVERSATION_TYPING_STARTED_INDICATOR",
      "CONVERSATION_TYPING_STOPPED_INDICATOR",
      "CONVERSATION_CLOSE_CONVERSATION"
    ],
    "message_types": [
      "StaticContentMessage"
    ],
    "message_format_types": [
      "Text"
    ],
    "user_verification": {
      "status": "Not Implemented",
      "description": "Only unauthenticated conversations are currently supported. User Verification is not yet implemented in the sample app."
    },
    "pre_chat": {
      "status": "Partially Implemented",
      "features": [
        "Pre-Chat UI support is added for all the form field types supported in the Embedded Service Deployment.",
        "Pre-Chat support is added to both display frequency configurations i.e. 'Every Conversation' or 'Every Session'.",
        "Hidden Pre-Chat support is not implemented in the app."
      ]
    },
    "session_continuity": {
      "status": "Limited Implementation", 
      "features": [
        "App supports session continuity for Messaging Conversations in the same browser tab only, i.e. reloading a page in the same tab should continue an open conversation.",
        "Read/Delivered acknowledgements are not rendered after reload in the sample app."
      ]
    }
  },
